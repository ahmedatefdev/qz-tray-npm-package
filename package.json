{
  "name": "qz-tray-noon",
  "version": "1.0.0",
  "description": "this is npm package thats will use for the noon projects ",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastfishio/qz-tray-noon.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
